  99:app_data_stream.c ****  *  NAME
 100:app_data_stream.c ****  *      streamSendRetryTimer
 101:app_data_stream.c ****  *
 102:app_data_stream.c ****  *  DESCRIPTION
 103:app_data_stream.c ****  *      Timer handler to retry sending next packet
 104:app_data_stream.c ****  *
 105:app_data_stream.c ****  *  RETURNS
 106:app_data_stream.c ****  *      Nothing.
 107:app_data_stream.c ****  *
 108:app_data_stream.c ****  *---------------------------------------------------------------------------*/
 109:app_data_stream.c **** static void streamSendRetryTimer(timer_id tid)
 110:app_data_stream.c **** {
 111:app_data_stream.c ****     if( tid == stream_send_retry_tid )
 112:app_data_stream.c ****     {
 113:app_data_stream.c ****         stream_send_retry_tid = TIMER_INVALID;
 114:app_data_stream.c ****         stream_send_retry_count++;
 115:app_data_stream.c ****         if( stream_send_retry_count < MAX_SEND_RETRIES )
 116:app_data_stream.c ****         {
 117:app_data_stream.c ****             StreamResendLastData();
 118:app_data_stream.c ****             stream_send_retry_tid =  TimerCreate(STREAM_SEND_RETRY_TIME, TRUE,
 119:app_data_stream.c ****                                                           streamSendRetryTimer);
 120:app_data_stream.c ****         }
 121:app_data_stream.c ****         else
 122:app_data_stream.c ****         {
 123:app_data_stream.c ****             stream_send_retry_count = 0;
 124:app_data_stream.c ****             StreamFlush();
 125:app_data_stream.c ****         }
 126:app_data_stream.c ****     }
 127:app_data_stream.c **** }
