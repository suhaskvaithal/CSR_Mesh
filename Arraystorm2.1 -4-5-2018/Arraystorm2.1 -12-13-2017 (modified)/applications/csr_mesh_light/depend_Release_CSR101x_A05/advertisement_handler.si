 421:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     addr->nap |=  BD_ADDR_NAP_RANDOM_TYPE_STATIC;
 422:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** }
