 152:app_data_stream.c **** /*-----------------------------------------------------------------------------*
 153:app_data_stream.c ****  *  NAME
 154:app_data_stream.c ****  *      sendNextPacket
 155:app_data_stream.c ****  *
 156:app_data_stream.c ****  *  DESCRIPTION
 157:app_data_stream.c ****  *      Forms a stream data packet with the current counter and sends it to
 158:app_data_stream.c ****  *      the stream receiver
 159:app_data_stream.c ****  *
 160:app_data_stream.c ****  *  RETURNS/MODIFIES
 161:app_data_stream.c ****  *      Nothing
 162:app_data_stream.c ****  *
 163:app_data_stream.c ****  *----------------------------------------------------------------------------*/
 164:app_data_stream.c **** static void sendNextPacket(void)
 165:app_data_stream.c **** {
 166:app_data_stream.c ****     uint16 data_pending, len;
 167:app_data_stream.c **** 
 168:app_data_stream.c ****     /* Stop retry timer */
 169:app_data_stream.c ****     stream_send_retry_count = 0;
 170:app_data_stream.c ****     TimerDelete(stream_send_retry_tid);
 171:app_data_stream.c ****     stream_send_retry_tid = TIMER_INVALID;
 172:app_data_stream.c **** 
 173:app_data_stream.c ****     data_pending = device_info_length+2 - tx_stream_offset;
 174:app_data_stream.c **** 
 175:app_data_stream.c ****     if( data_pending )
 176:app_data_stream.c ****     {
 177:app_data_stream.c ****         len = (data_pending > STREAM_DATA_BLOCK_SIZE_MAX)? STREAM_DATA_BLOCK_SIZE_MAX:data_pending;
 178:app_data_stream.c **** 
 179:app_data_stream.c ****         /* Send the next packet */
 180:app_data_stream.c ****         StreamSendData(&device_info[tx_stream_offset], len);
 181:app_data_stream.c ****         tx_stream_offset += len;
 182:app_data_stream.c **** 
 183:app_data_stream.c ****         stream_send_retry_tid = TimerCreate(100 * MILLISECOND, TRUE,
 184:app_data_stream.c ****                                                        streamSendRetryTimer);
 185:app_data_stream.c ****     }
 186:app_data_stream.c ****     else
 187:app_data_stream.c ****     {
 188:app_data_stream.c ****         /* Send flush to indicate end of stream */
 189:app_data_stream.c ****         StreamFlush();
 190:app_data_stream.c ****     }
 191:app_data_stream.c **** }
