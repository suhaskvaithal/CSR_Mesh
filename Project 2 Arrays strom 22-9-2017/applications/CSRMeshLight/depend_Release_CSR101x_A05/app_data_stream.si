 262:app_data_stream.c **** extern void handleCSRmeshDataBlockInd(CSR_MESH_STREAM_EVENT_T *p_event)
 263:app_data_stream.c **** {
 264:app_data_stream.c ****     switch(p_event->data[0])
 265:app_data_stream.c ****     {
 266:app_data_stream.c ****         case CSR_DEVICE_INFO_REQ:
 267:app_data_stream.c ****         {
 268:app_data_stream.c ****             /* Set the source device ID as the stream target device */
 269:app_data_stream.c ****             StreamStartSender(p_event->common_data.source_id );
 270:app_data_stream.c ****             tx_stream_offset = 0;
 271:app_data_stream.c ****             /* Set the opcode to CSR_DEVICE_INFO_RSP */
 272:app_data_stream.c ****             device_info[0] = CSR_DEVICE_INFO_RSP;
 273:app_data_stream.c **** 
 274:app_data_stream.c ****             /* start sending the data */
 275:app_data_stream.c ****             sendNextPacket();
 276:app_data_stream.c ****         }
 277:app_data_stream.c ****         break;
 278:app_data_stream.c **** 
 279:app_data_stream.c ****         case CSR_DEVICE_INFO_RESET:
 280:app_data_stream.c ****         {
 281:app_data_stream.c ****             /* Reset the device info */
 282:app_data_stream.c ****             device_info_length = sizeof(DEVICE_INFO_STRING);
 283:app_data_stream.c ****             device_info[0] = CSR_DEVICE_INFO_RSP;
 284:app_data_stream.c ****             device_info[1] = device_info_length;
 285:app_data_stream.c ****             MemCopy(&device_info[2], DEVICE_INFO_STRING,
 286:app_data_stream.c ****                                                    sizeof(DEVICE_INFO_STRING));
 287:app_data_stream.c ****         }
 288:app_data_stream.c ****         break;
 289:app_data_stream.c **** 
 290:app_data_stream.c ****         default:
 291:app_data_stream.c ****         break;
 292:app_data_stream.c ****     }
 293:app_data_stream.c **** }
